{
  "compilerOptions": {
    "target": "es5",
    "module": "es2022",
    "outDir": "./dist",
    "strict": true,
    "noUnusedLocals": true,
    "noUnusedParameters": false,
    "noImplicitReturns": true,
    "noFallthroughCasesInSwitch": true,
    "esModuleInterop": true,
},
  "include": ["lib/**/*"],
  "exclude": ["node_modules", "**/*.spec.ts"],
}